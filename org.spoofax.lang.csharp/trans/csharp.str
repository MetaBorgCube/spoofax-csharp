module csharp

imports
  
  libstratego-lib
  libstratego-gpp
  libstratego-aterm
  include/CSharp
  lib/editor-common.generated

rules
 
  editor-analyze:
  	(ast, path, project-path) -> (ast, [], [], [])
  
  debug-generate-aterm:
    (selected, position, ast, path, project-path) -> (filename, ast)
    with
      filename := <guarantee-extension(|"aterm")> path
