signature
  constructors
    Program   : Namespace
    Namespace : Namespace
    Using     : Namespace
    Class     : Namespace
    Function  : Namespace
    Field     : Namespace
    Variable  : Namespace


rules

  nabl-def-site(cpaths, spaths|lang, elems, tasks, paths, states) =
    ?Namespace(x, _)
    ; Namespace(
        nabl-def(
          cpaths
        , spaths
        | lang
        , elems
        , tasks
        , paths
        , Namespace()
        , NonUnique()
        , Current()
        , [Namespace(), Class()]
        )
      , id
      )


