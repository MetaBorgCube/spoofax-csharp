module old-names

imports
  nbl/-
  include/CSharp
  desugar
  types


signature
  constructors
    Program   : Namespace
    Namespace : Namespace
    Using     : Namespace
    Class     : Namespace
    Function  : Namespace
    Field     : Namespace
    Variable  : Namespace


rules

  nabl-def-site(cpaths, spaths|lang, elems, tasks, paths) =
    ?Namespace(x, _)
    ; id
    ; Namespace(
        nabl-def(
          cpaths
        , spaths
        | lang
        , elems
        , tasks
        , paths
        , Namespace()
        , NonUnique()
        , Current()
        , [Namespace(), Class()]
        )
      , id
      )

  nabl-prop-site(|lang, elems, tasks) =
    ?Namespace(x, _)
    ; Namespace(
        nabl-props(|lang, elems, tasks, [])
      , id
      )


