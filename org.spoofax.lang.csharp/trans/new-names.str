  	]));
    vdebug(!"leave ")
    
  
  // VarRef(x)
  nbl-use-site(|lang, elems, tasks, paths, deps) =
    ?VarRef(x);
    vdebug(!"enter ");
    VarRef(nbl-use(|lang, elems, tasks, paths, deps, [
    	Candidate(Variable(), [], Current()), 
    	Candidate(Field(), [], Current())
  	]));
    vdebug(!"leave ")
  
  // Assign(x, _)
  nbl-use-site(|lang, elems, tasks, paths, deps) =
    ?Assign(x, _);
    vdebug(!"enter ");
    Assign(nbl-use(|lang, elems, tasks, paths, deps, [
    	Candidate(Variable(), [], Current()), 
    	Candidate(Field(), [], Current())
  	]), id);
    vdebug(!"leave ")

